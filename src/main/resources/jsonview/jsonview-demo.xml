<jsonview-configuration>
	<jsonview-package namespace="namespace">
		<jsonview id="test-user">
			<class data="user" class-name="test.User">
				<ignore-property name="likes" />
				<ignore-property name="map" />
			</class>
		</jsonview>
		<jsonview id="user-detail">
			<property data="#strName"></property>
			<property data="id"></property>
			<property data="name"></property>
			<array data="likes">
				<property data="name"></property>
			</array>
		</jsonview>
	</jsonview-package>
</jsonview-configuration>
