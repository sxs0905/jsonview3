<jsonview-configuration>
	<jsonview-package namespace="namespace">
		<jsonview id="test-user">
			<array data="users">
				<import id="user-detail"/>
			</array>
		</jsonview>

		<jsonview id="user-detail">
			<property data="#strName"></property>
			<property data="id"></property>
			<property data="name"></property>
			<array data="likes">
				<property data="name"></property>
			</array>
			<property-date data="birthday"/>
		</jsonview>

		<jsonview id="test" data="user" for-class="test.User">
			<ignore-property data="likes" />
			<ignore-property data="map" />
			<property data="name" alias="userName"></property>
		</jsonview>
	</jsonview-package>
</jsonview-configuration>
