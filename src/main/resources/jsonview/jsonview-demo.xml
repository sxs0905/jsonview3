<jsonview-configuration>
	<jsonview-package namespace="namespace">
		<jsonview id="id">
			<array bind="users">
				<property bind="id"></property>
				<property bind="name"></property>
			</array>
		</jsonview>
		<jsonview id="test-str">
			<array bind="str"></array>
		</jsonview>
	</jsonview-package>
</jsonview-configuration>
